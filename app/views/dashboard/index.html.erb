    <h1>Logged in!</h1>
    <%= link_to "Log Out", destroy_user_session_path, data: {
            turbo_method: :delete}, class: "btn btn-primary btn-md" %>

    <%= form_with model: Tweet.new, url: tweets_path, method: :post, local: true do |f|  %>
        <%= f.text_field :body %>
        <%= f.submit %>
    <% end %>

    <ul>
        <% Tweet.find_each do |tweet| %>
            <li>
                <%= tweet.body %>
                <%= link_to "delete", tweet_path(tweet.id), data: {
            turbo_method: :delete } %>
            </li>
        <% end %>
    </ul>